
main x y = { three (add (S (S y)) (S x))
}

add x y = { case x of 
  S v -> S (add v y) ;
  Z -> y ;
}

three x = { case x of
  S y -> case y of 
    S z -> case z of   
      S u -> case u of
        S l -> False;
        Z -> True;;
      Z -> False;;
    Z -> False;;
  Z -> False ;
}

